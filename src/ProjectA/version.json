{
  "$schema": "https://raw.githubusercontent.com/dotnet/Nerdbank.GitVersioning/main/src/NerdBank.GitVersioning/version.schema.json",
  "version": "1.0.1-rc",
  "assemblyVersion": {
    "precision": "revision"
  },
  "publicReleaseRefSpec": [
    "^refs/heads/main$"
  ],
  "release": {
    "tagName": "projectA-v{version}{prereleaseVersion}",
    "branchName": "release/projectA-v{version}{prereleaseVersion}",
    "versionIncrement": "build",
    "firstUnstableTag": "rc"
  },
  "pathFilters": [
    "./"
  ],
  "inherit": true
}